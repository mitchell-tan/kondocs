{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Kondo Docs",
  "colors": {
    "primary": "#000000",
    "light": "#FFFFFF",
    "dark": "#000000"
  },
  "favicon": "/favicon.svg",
  "appearance": {
    "default": "light",
    "strict": true
  }, 
  "navigation": {
    "groups": [
    {
      "group": "Getting Started",
      "pages": [
        "quickstart",
        "getting-started/change-login",
        "getting-started/sharing-access"    
        ]
    },
    {
      "group": "Archiving"
    } 
    ]
  },
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://mintlify.com/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Community",
          "href": "https://mintlify.com/community",
          "icon": "slack"
        },
        {
          "anchor": "Blog",
          "href": "https://mintlify.com/blog",
          "icon": "newspaper"
        }
      ]
  },
  "logo": {
    "light": "kondodocs.svg",
    "dark": "/kondodocslogo.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Contact Us",
        "href": "mailto:mitchell@trykondo.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Open App",
      "href": "https://app.trykondo.com"
    }
  },
  "footer": {
    "socials": {
      "linkedin": "https://www.linkedin.com/in/mitchell-tan/"
    }
  },
  "style": "/style.css"
}